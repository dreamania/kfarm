 <div class="row-fluid">
    <!-- block -->
    <div class="block">
        <div class="navbar navbar-inner block-header">
            <div class="muted pull-left"><%= current_user.name %>님의 멘토링 목록</div>
        </div>
        <div class="block-content collapse in">
            <div class="span12">
                <table cellpadding="0" cellspacing="0" border="0" class="table table-striped table-bordered" id="example">
                <thead>
                  <tr>
                    <th>#</th>
                    <th>프로그램 이름</th>
                    <th>종류</th>
                    <th>시작일</th>
                    <th>교육시간</th>
                    <th>필요인원</th>
                    <th>일당</th>
                    <th>관리</th>
                  </tr>
                </thead>
                <tbody>
                  <% @jobs.each do |job| %>
                  <tr class="">
                    <td><%= job.id %></td>
                    <td class="center"><%= job.title %></td>
                    <td class="center"><%= job.category %></td>
                    <td class="center"><%= job.started_at.strftime("%Y. %m. %d")%></td>
                    <td class="center"><%= job.working_time %></td>
                    <td class="center"><%= job.workers_count %></td>
                    <td class="center"><%= job.pay %></td>
                    <td class="center">
                      <%= link_to "상세정보", mentor_job_path(job) %>
                      <%= link_to "수정", edit_mentor_job_path(job) %>
                      <a href="#">멘티</a>
                    </td>
                  </tr>
                  <% end %>
                </tbody>
              </table>
            </div>
            <%= link_to "멘토링 프로그램 등록", new_mentor_job_path, class: "btn btn-primary"%>
        </div>
    </div>
    <!-- /block -->
</div>